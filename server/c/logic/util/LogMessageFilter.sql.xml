			passed back and forth to determine log filtering settings.
		</Comment>
		<Sql>
			-- Nothing here
		</Sql>
		<Input name="LogFile" type="twine"/>
		<Input name="StartId" type="int"/>
		<Input name="Forward" type="int"/>
		<Input name="GetMax" type="int"/>
		<Input name="MachineName" type="twine"/>
		<Input name="AppName" type="twine"/>
		<Input name="ThreadID" type="int"/>
		<Input name="SUID" type="twine"/>
		<Input name="Message" type="twine"/>
		<Input name="Panic" type="int"/>
		<Input name="Error" type="int"/>
		<Input name="Warn" type="int"/>
		<Input name="Info" type="int"/>
		<Input name="Debug" type="int"/>
		<Input name="Trace" type="int"/>
		<Input name="SqlTrace" type="int"/>
		<Input name="DispId" type="int"/>
		<Input name="DispDate" type="int"/>
		<Input name="DispMachine" type="int"/>
		<Input name="DispApp" type="int"/>
		<Input name="DispThread" type="int"/>
		<Input name="DispFile" type="int"/>
		<Input name="DispLine" type="int"/>
		<Input name="DispChannel" type="int"/>
		<Input name="ShowColIcon" type="int"/>
