		<Input name="ShowColLine" type="int"/>
		<Input name="ShowColChannel" type="int"/>
		<Input name="ShowColMessage" type="int"/>

		<Input name="FirstRow" type="int"/>
		<Input name="LastRow" type="int"/>

		<!-- These are some stats about the log file itself. -->
		<Input name="LogFileOldestID" type="int"/>
		<Input name="LogFileNewestID" type="int"/>
		<Input name="FilterMatchCount" type="int"/>

		<!-- These are used when the user is searching for a specific record in the logs -->
		<Input name="FindByID" type="int"/>
		<Input name="FindByDate" type="twine"/>
		<Input name="FindByMsg" type="twine"/>
